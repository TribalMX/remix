<script src="http://code.jquery.com/jquery-1.8.3.min.js"></script>
<script type="text/javascript">
  $(document).ajaxError(function(event, request, settings){
    if(request.status == 401) {
      window.location.replace('/');
    }
  //JQM Configuration
  }).bind('mobileinit', function(){
    $.extend($.mobile, {
      defaultPageTransition: "none"
    });
  });
</script>
<script src="http://code.jquery.com/mobile/1.2.1/jquery.mobile-1.2.1.min.js"></script>
<script type="text/javascript" src="javascripts/lib/tolito-1.0.5.min.js"></script>
<script src="javascripts/lib/async.js"></script>
<script src="javascripts/lib/uploader.js?10"></script>
<script src="javascripts/functions.js?r=10"></script>
<script src="javascripts/app.js?r=17"></script>
</body>
</html>
